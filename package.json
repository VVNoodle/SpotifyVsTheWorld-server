{
  "name": "express-ws",
  "version": "0.0.0",
  "private": true,
  "description": "Example for using serve-grip with WebSockets-over-HTTP in Express",
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "dev": "nodemon",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Egan Bisma <cendoloverboba@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/ioredis": "^4.17.9",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.8",
    "@types/ws": "^7.4.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fastify": "^3.9.2",
    "fastify-cors": "^5.1.0",
    "fastify-rate-limit": "^5.0.1",
    "fastify-schedule": "^1.0.0",
    "handy-redis": "^2.2.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "redis": "^3.0.2",
    "toad-scheduler": "^1.0.2",
    "ts-node": "9.1.1",
    "tslib": "^2.0.3",
    "typescript": "4.0.5",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "tsutils": "3.17.0"
  }
}
