{
  "name": "express-ws",
  "version": "0.0.0",
  "private": true,
  "description": "Example for using serve-grip with WebSockets-over-HTTP in Express",
  "engines": {
    "node": "12.9.1"
  },
  "engineStrict": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "dev": "nodemon",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Egan Bisma <cendoloverboba@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fanoutio/grip": "3.0.3",
    "@fanoutio/serve-grip": "1.1.7-beta.2",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "nodemon": "^2.0.6",
    "ts-node": "9.1.1",
    "tslib": "^2.0.3",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "tsutils": "3.17.0"
  }
}
